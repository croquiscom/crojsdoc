{
  "name": "crojsdoc",
  "description": "A documentation generator for JavaScript and CoffeeScript",
  "version": "0.8.17",
  "keywords": [
    "documentation",
    "doc",
    "jsdoc",
    "coffeescript"
  ],
  "homepage": "https://croquiscom.github.com/crojsdoc",
  "bugs": {
    "url": "https://github.com/croquiscom/crojsdoc/issues"
  },
  "author": "Sangmin Yoon <sangmin.yoon@croquis.com>",
  "license": "MIT",
  "bin": {
    "crojsdoc": "./bin/crojsdoc"
  },
  "scripts": {
    "build": "coffee -c -o lib src",
    "doc": "node bin/crojsdoc",
    "test": "NODE_ENV=test mocha --compilers coffee:coffee-script/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/croquiscom/crojsdoc.git"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "dependencies": {
    "fs.extra": "^1.3.2",
    "glob": "^7.0.3",
    "inflect": "^0.3.0",
    "jade": "^1.11.0",
    "js-yaml": "^3.6.1",
    "jsdoctypeparser": "^2.0.0-alpha-8",
    "lodash": "^4.13.1",
    "marked": "^0.3.5",
    "optparse": "^1.0.5",
    "walkdir": "^0.0.11"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-coverage": "^1.0.1",
    "coffee-script": "^1.10.0",
    "crojsdoc-plugin-auto-namespace": "^0.1.1",
    "crojsdoc-plugin-underscore-private": "^0.1.0",
    "mocha": "^2.5.3"
  }
}
